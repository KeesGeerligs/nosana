{
    "version": "0.1",
    "type": "container",
    "meta": {
      "trigger": "benchmark"
    },
    "ops": [
      {
        "type": "container/run",
        "id": "generic_benchmark",
        "args": {
          "cmd": [
            "/bin/sh",
            "-c",
            "echo \"Running bench...\"; bench --benchmark generic --check-specs --version 0.0.0"
          ],
          "image": "docker.io/nosana/sd-auto-bench:0.2.0.9.2",
          "gpu": true
        },
        "results": {
          "results_generic_benchmark": "(?<=Generic benchmark results: ).*",
          "results_system_specs": "(?<=system specs: ).*"

        }
      }
    ]
  }